Cell["Rules of differentiation", "Subsection",
 CellChangeTimes->{{3.886499693991352*^9, 
  3.886499696091084*^9}},ExpressionUUID->"a3504f0f-36c1-4446-9a56-\
ef06d13f48fa"],

Cell["\<\
You can 'simulate' the rules of differentiation like product-rule and \
chain-rule.\
\>", "Text",ExpressionUUID->"ae930115-bc26-44e3-9bd9-2eb3c5e6b09d"],

Cell[BoxData[
 RowBox[{"Clear", "[", 
  RowBox[{"f", ",", "g"}], "]"}]], "Input",
 CellLabel->"In[8]:=",ExpressionUUID->"09fa5d7b-dfd3-44e3-b7e2-7e2932dc473a"],

Cell["The product-rule:", "Text",ExpressionUUID->"bef6af67-4c0b-4c74-b994-7818b58c2a36"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "x"], 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"f", "[", "x", "]"}], " ", 
    RowBox[{"g", "[", "x", "]"}]}], ")"}]}]], "Input",
 CellLabel->"In[9]:=",ExpressionUUID->"a7e27af8-d38b-4e1c-a7e9-b08fc4fb7c95"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "x", "]"}], " ", 
   RowBox[{
    SuperscriptBox["f", "\[Prime]",
     MultilineFunction->None], "[", "x", "]"}]}], "+", 
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], " ", 
   RowBox[{
    SuperscriptBox["g", "\[Prime]",
     MultilineFunction->None], "[", "x", "]"}]}]}]], "Output",
 CellChangeTimes->{3.886499711241065*^9},
 CellLabel->"Out[9]=",ExpressionUUID->"99f735fa-3df4-4ca1-ad5e-6b6b40c183d8"]
}, Open  ]],

Cell["Quotient-rule:", "Text",ExpressionUUID->"aedaa69d-f4e8-4731-a303-92f9dd648df0"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "x"], 
  FractionBox[
   RowBox[{"f", "[", "x", "]"}], 
   RowBox[{"g", "[", "x", "]"}]]}]], "Input",
 CellLabel->"In[10]:=",ExpressionUUID->"ca1b4611-440a-4e81-8664-5e3c8998c2be"],

Cell[BoxData[
 RowBox[{
  FractionBox[
   RowBox[{
    SuperscriptBox["f", "\[Prime]",
     MultilineFunction->None], "[", "x", "]"}], 
   RowBox[{"g", "[", "x", "]"}]], "-", 
  FractionBox[
   RowBox[{
    RowBox[{"f", "[", "x", "]"}], " ", 
    RowBox[{
     SuperscriptBox["g", "\[Prime]",
      MultilineFunction->None], "[", "x", "]"}]}], 
   SuperscriptBox[
    RowBox[{"g", "[", "x", "]"}], "2"]]}]], "Output",
 CellChangeTimes->{3.886499719481902*^9},
 CellLabel->"Out[10]=",ExpressionUUID->"05683833-50f5-484a-8124-f15169d34833"]
}, Open  ]],

Cell["And the chain-rule:", "Text",ExpressionUUID->"9f97538b-05aa-4f9b-8520-4746d145f35b"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "x"], 
  RowBox[{"f", "[", 
   RowBox[{"g", "[", "x", "]"}], "]"}]}]], "Input",
 CellLabel->"In[11]:=",ExpressionUUID->"e8724d02-1a94-4c46-b424-f678e31713b2"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   SuperscriptBox["f", "\[Prime]",
    MultilineFunction->None], "[", 
   RowBox[{"g", "[", "x", "]"}], "]"}], " ", 
  RowBox[{
   SuperscriptBox["g", "\[Prime]",
    MultilineFunction->None], "[", "x", "]"}]}]], "Output",
 CellChangeTimes->{3.886499741862545*^9},
 CellLabel->"Out[11]=",ExpressionUUID->"8b8c649f-ffba-4349-a5a4-9d2c6729f650"]
}, Open  ]]
}, Open  ]],

