Cell[CellGroupData[{

Cell["Examples", "Subsection",ExpressionUUID->"6f975f19-1970-4115-96d4-32b868fbf295"],

Cell[TextData[{
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " can produce derivatives of all standard mathematical functions.Here you \
may use symbolic notation for derivatives in stead of D. For example ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["\[PartialD]", "x"], 
   RowBox[{"Sin", "[", "x", "]"}]}]],ExpressionUUID->
  "46d9c66b-7f79-4bac-92d6-b27034773c7d"],
 " in stead of D[Sin[x],x] and in the case of 2nd order derivatives ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    SubscriptBox["\[PartialD]", 
     RowBox[{"{", 
      RowBox[{"x", ",", "2"}], "}"}]], 
    RowBox[{"Sin", "[", "x", "]"}]}], " "}]],ExpressionUUID->
  "200b5843-f462-4837-a78f-b0ccbbfd0e1d"],
 "in stead of D[Sin[x],x,x]."
}], "Text",ExpressionUUID->"f009747a-c8d8-412c-be7b-bce179df738e"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "x"], 
  RowBox[{"Sin", "[", "x", "]"}]}]], "Input",ExpressionUUID->"e4638f0d-8874-\
4725-a6eb-db61e99961ba"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "y"], 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"3", " ", 
     SuperscriptBox["y", "3"]}], "+", 
    RowBox[{"7", " ", 
     SuperscriptBox["y", "2"]}], "-", 
    RowBox[{"2", " ", "y"}], "+", "8"}], ")"}]}]], "Input",ExpressionUUID->\
"7b8cd2a9-4eb9-47ef-a289-53224cc3514d"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "x"], 
  RowBox[{"ArcTan", "[", "x", "]"}]}]], "Input",ExpressionUUID->"7947f681-\
e896-44b6-bb61-01cc3974d107"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", "x"], 
  RowBox[{"Log", "[", "x", "]"}]}]], "Input",ExpressionUUID->"3cfbfed1-daf9-\
48b2-915f-3bd3c87815f1"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", 
   RowBox[{"{", 
    RowBox[{"x", ",", "2"}], "}"}]], 
  RowBox[{"Log", "[", "x", "]"}]}]], "Input",ExpressionUUID->"281558c2-8278-\
4336-a19b-84f8afb5c2a3"],

Cell[BoxData[
 RowBox[{
  SubscriptBox["\[PartialD]", 
   RowBox[{"{", 
    RowBox[{"x", ",", "3"}], "}"}]], 
  RowBox[{"Log", "[", "x", "]"}]}]], "Input",ExpressionUUID->"ac1871b7-62c1-\
4622-98e9-bbf068f15fc5"],

Cell["\<\
It is even possible to use the familiar 'prime' for differentiation:\
\>", "Text",
 CellChangeTimes->{
  3.50572375103125*^9},ExpressionUUID->"741f5011-413a-4ce8-ba08-8b52962a80a6"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["Sin", "\[Prime]",
   MultilineFunction->None], "[", "x", "]"}]], "Input",ExpressionUUID->\
"ba0b8c3a-55d3-4d44-a757-3ef87018d4b3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{
   SuperscriptBox["x", "2"], " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", "x"}]]}]}]], "Input",ExpressionUUID->"316b2918-fbcd-4609-\
a6cf-e3b5172fe1ef"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["f", "\[Prime]",
   MultilineFunction->None], "[", "x", "]"}]], "Input",ExpressionUUID->\
"d39d88ab-19cf-4fcf-9245-a30c0b8262b0"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["f", "\[Prime]\[Prime]",
   MultilineFunction->None], "[", "x", "]"}]], "Input",ExpressionUUID->\
"26b135d6-d123-48f2-96a0-d4db1224ebcd"],

Cell[BoxData[
 RowBox[{
  SuperscriptBox["f", 
   TagBox[
    RowBox[{"(", "3", ")"}],
    Derivative],
   MultilineFunction->None], "[", "x", "]"}]], "Input",ExpressionUUID->\
"0816fe5a-c11f-44ac-9e02-214de080f4c3"]
