Cell[TextData[{
 "There is a way out, using Evaluate again, in order to force ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " to first evaluate the derivative, before assigning a value to ",
 StyleBox["x",
  FontSlant->"Italic"],
 ":"
}], "Text",
 CellChangeTimes->{{3.50572360421875*^9, 3.50572362478125*^9}, {
  3.505724094421875*^9, 
  3.50572415546875*^9}},ExpressionUUID->"40d1112e-479a-4b19-877f-\
58a47bb5e960"],

Cell[BoxData[
 RowBox[{
  RowBox[{"dsin", "[", "x_", "]"}], ":=", 
  RowBox[{"Evaluate", "[", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"Sin", "[", "x", "]"}], ",", "x"}], "]"}], "]"}]}]], "Input",
